    "doctoc": "^1.0.0",
    "growly": "^1.3.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-clean-css": "^2.0.7",
    "gulp-concat": "^2.6.0",
    "gulp-header": "^1.7.1",
    "gulp-if": "^2.0.0",
    "gulp-ignore": "^2.0.1",
    "gulp-jshint": "^2.0.0",
    "gulp-less": "^3.1.0",
    "gulp-minify-html": "^1.0.6",
    "gulp-rename": "^1.2.2",
    "gulp-rev": "^7.0.0",
    "gulp-sass": "^2.3.1",
    "gulp-sourcemaps": "^2.0.0-alpha",
    "gulp-uglify": "^1.5.3",
    "gulp-useref": "^3.1.0",
    "html5": "^1.0.5",
    "http-server": "^0.6.1",
    "istanbul": "^0.4.3",
    "jasmine-core": "^2.3.4",
    "jasmine-jquery": "^2.1.1",
    "jquery": "^2.2.3",
    "jshint": "^2.9.2",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.1.12",
    "karma-commonjs": "0.0.13",
    "karma-coverage": "^0.5.5",
    "karma-cucumber-js": "^0.2.0",
    "karma-htmlfile-reporter": "^0.2.2",
    "karma-jasmine": "^0.3.5",
    "karma-jasmine-html-reporter": "^0.2.0",
    "karma-jshint-preprocessor": "0.0.6",
    "karma-junit-reporter": "^0.2.2",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.4",
    "karma-ng-html2js-preprocessor": "^0.2.1",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sinon": "^1.0.5",
    "lazypipe": "^1.0.1",
    "mocha": "^2.5.3",
    "nodemon": "^1.0.15",
    "phantomjs-prebuilt": "^2.1.7",
    "protractor": "^2.1.0",
    "reactify": "^0.5.1",
    "shelljs": "^0.2.6",
    "sinon": "^1.17.4",
    "uglify-js": "^2.4.8",
    "watchify": "^0.6.2",
    "zombie": "^4.2.1"
  },
  "scripts": {
    "postinstall2": "bower prune && bower install && bower update",
    "postinstall": "bower install",
    "prestart": "npm install",
    "start": "http-server -a localhost -p 8080 -c-1",
    "pretest": "npm install",
    "test": "node_modules/.bin/karma start tests/karma.conf.js",
    "mocha": "node_modules/.bin/karma start tests/karma.mocha.conf.js",
    "test-single-run": "node_modules/.bin/karma start tests/karma.conf.js  --single-run",
    "test-single-run-no-watch": "node_modules/.bin/karma start tests/karma.conf.js --single-run --no-auto-watch",
    "posttest": "npm run build",
    "unit-test": "node_modules/.bin/karma start tests/karma.conf.js",
    "unit-test-bdd": "node_modules/.bin/karma start tests/karma.bdd.conf.js",
    "unit-test-code-coverage-report": "node_modules/.bin/karma start tests/karma.coverage.conf.js",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor tests/e2e/e2e/protractor.conf.js",
    "protractor-debug": "protractor --elementExplorer tests/e2e/e2e/protractor.conf.js",
    "update-index-async": "node -e \"require('shelljs/global'); sed('-i', /\\/\\/@@NG_LOADER_START@@[\\s\\S]*\\/\\/@@NG_LOADER_END@@/, '//@@NG_LOADER_START@@\\n' + sed(/sourceMappingURL=angular-loader.min.js.map/,'sourceMappingURL=bower_components/angular-loader/angular-loader.min.js.map','app/bower_components/angular-loader/angular-loader.min.js') + '\\n//@@NG_LOADER_END@@', 'app/index-async.html');\"",
    "build": "npm run build-js && npm run build-less",
    "build-js": "browerify -t reactify app/js/main.js | uglifyjs -mc > static/bundle.js",
    "build-less": "lessc app/less/main.less static/main.css",
    "watch": "npm run watch-js & npm run watch-less & npm run watch-server",
    "watch-js": "watchify app/js/main.js -t reactify -o static/bundle.js -dv",
    "watch-less": "nodemon --watch app/less/*.less --ext less --exec 'npm run build-less'",
    "watch-server": "nodemon --ignore app --ignore static server.js",
    "deploy": "./bin/deploy.sh"
  }
}
