{
  "name": "angular-seed",
  "config": {
    "reporter": "xunit",
    "reporter2": "spec"
  },
  "private": true,
  "version": "0.0.0",
  "description": "A starter proyject for AngularJS",
  "repository": "https://github.com/angular/angular-seed",
  "license": "MIT",
  "scripts": {
    "prestart": "npm install",
    "start": "http-server -a localhost -p 8080 -c-1",
    "test2": "mocha test/",
    "test3": "mocha test/ --reporter $npm_package_config_reporter # win %",
    "test:xunit": "npm run test -- --reporter xunit",
    "test:xunit2": "npm run test -- --reporter xunit",
    "test:dev": "npm run test --angular-seed:reporter2=spec",
    "mocha": "node_modules/.bin/karma start tests/karma.mocha.conf.js",
    "pretest": "npm install",
    "test": "node_modules/.bin/karma start tests/karma.conf.js",
    "test-single-run": "node_modules/.bin/karma start tests/karma.conf.js  --single-run",
    "test-single-run-no-watch": "node_modules/.bin/karma start tests/karma.conf.js --single-run --no-auto-watch",
    "posttest": "npm run build",
    "unit-test": "node_modules/.bin/karma start tests/karma.conf.js",
    "unit-test-bdd": "node_modules/.bin/karma start tests/karma.cucumber.conf.js",
    "unit-test-code-coverage-report": "node_modules/.bin/karma start tests/karma.coverage.conf.js",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor tests/e2e/e2e/protractor.conf.js",
    "protractor-debug": "protractor --elementExplorer tests/e2e/e2e/protractor.conf.js",
    "update-index-async": "node -e \"require('shelljs/global'); sed('-i', /\\/\\/@@NG_LOADER_START@@[\\s\\S]*\\/\\/@@NG_LOADER_END@@/, '//@@NG_LOADER_START@@\\n' + sed(/sourceMappingURL=angular-loader.min.js.map/,'sourceMappingURL=node_modules/angular-loader/angular-loader.min.js.map','app/node_modules/angular-loader/angular-loader.min.js') + '\\n//@@NG_LOADER_END@@', 'app/index-async.html');\"",
    "build": "npm run build-js && npm run build-less",
    "build-js": "browerify -t reactify app/js/main.js | uglifyjs -mc > static/bundle.js",
    "build-less": "lessc app/less/main.less static/main.css",
    "watch": "npm run watch-js & npm run watch-less & npm run watch-server",
    "watch-js": "watchify app/js/main.js -t reactify -o static/bundle.js -dv",
    "watch-less": "nodemon --watch app/less/*.less --ext less --exec 'npm run build-less'",
    "watch-server": "nodemon --ignore app --ignore static server.js",
    "deploy": "bash ./bin/deploy.sh"
  },
  "dependencies": {
    "angular": "1.6.1",
    "angular-loader": "1.6.1",
    "angular-mocks": "1.6.1",
    "angular-resource": "1.6.1",
    "angular-route": "1.6.1",
    "bootstrap": "3.3.7",
    "growly": "1.3.0",
    "html5": "1.0.5",
    "html5-boilerplate": "5.3.0",
    "jasmine-jquery": "2.1.1",
    "jquery": "3.1.1"
  },
  "devDependencies": {
    "assert": "1.4.1",
    "browserify": "13.1.1",
    "chai": "3.5.0",
    "cucumber": "1.3.1",
    "doctoc": "1.2.0",
    "http-server": "0.9.0",
    "istanbul": "0.4.5",
    "jasmine-core": "2.5.2",
    "jasmine-jquery": "2.1.1",
    "jshint": "2.9.4",
    "karma": "1.3.0",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-commonjs": "1.0.0",
    "karma-coverage": "1.1.1",
    "karma-cucumber-js": "1.0.0",
    "karma-htmlfile-reporter": "0.3.4",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-jshint-preprocessor": "0.0.6",
    "karma-junit-reporter": "1.2.0",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.1",
    "karma-ng-html2js-preprocessor": "1.0.0",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-read-json": "1.1.0",
    "karma-sinon": "1.0.5",
    "less": "2.7.1",
    "mocha": "3.2.0",
    "nodemon": "1.11.0",
    "phantomjs-prebuilt": "2.1.14",
    "protractor": "4.0.14",
    "protractor-jasmine2-html-reporter": "0.0.6",
    "reactify": "1.1.1",
    "shelljs": "0.7.5",
    "sinon": "1.17.6",
    "watchify": "3.8.0",
    "zombie": "5.0.5"
  }
}
